{
  "name": "don-claudio-bot",
  "version": "1.0.0",
  "description": "WhatsApp-based multi-user AI assistant service with OpenClaw",
  "type": "module",
  "scripts": {
    "dev": "tsx onboarding/src/index.ts",
    "build": "tsc",
    "start": "node onboarding/dist/index.js",
    "docker:build": "docker build -t don-claudio-bot -f docker/Dockerfile .",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f"
  },
  "dependencies": {
    "openclaw": "2026.1.30"
  },
  "devDependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "onboarding"
  ],
  "engines": {
    "node": ">=22.0.0"
  }
}
